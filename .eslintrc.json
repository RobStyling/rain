{
    "esversion": 7,
"parserOptions": {
    "ecmaVersion":6
},
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
    "rules": {
        "quotes": [
            2,
            "double"
        ],
        "arrow-parens": "off",
        "no-console": "off",
        "linebreak-style": ["error", "windows"]
    }
}
